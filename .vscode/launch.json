    "version": "0.2.0",
    "configurations": [
        {
            "name": "strumok",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/lib/main.dart"
        },
        {
            "name": "strumok external lib",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/lib/main.dart",
            "toolArgs": [
                "--dart-define",
                "FFI_SUPPLIER_LIBS_DIR=../suppliers/target/release/",
                "--dart-define",
                "FFI_SUPPLIER_LIB_NAME=strumok_suppliers",
            ],
            "env": {
                "RUST_LOG": "info"
            }
        },
    ]
}